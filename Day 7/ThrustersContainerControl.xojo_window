	#tag EndProperty


	#tag Constant, Name = DefaultProgram, Type = String, Dynamic = False, Default = \"3\x2C23\x2C3\x2C24\x2C1002\x2C24\x2C10\x2C24\x2C1002\x2C23\x2C-1\x2C23\x2C\n101\x2C5\x2C23\x2C23\x2C1\x2C24\x2C23\x2C23\x2C4\x2C23\x2C99\x2C0\x2C0", Scope = Public
	#tag EndConstant


