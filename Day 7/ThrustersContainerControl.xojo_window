#tag WindowCode
	#tag Method, Flags = &h0
		Sub AutoCalibrateThrusters(Reset As Boolean = True)
		  Var Phases() As Integer = Array(0, 1, 2, 3, 4)
		  
		  If Reset Then
		    HighestOutputSignal = 0
		    BestConfiguration.RemoveAllRows
		    PendingPermutations.RemoveAllRows
		  End If
		  
		  BestConfiguration.AddRow 3
		  BestConfiguration.AddRow 1
		  BestConfiguration.AddRow 2
		  
		  For Each Phase As Integer In Phases
		    If BestConfiguration.IndexOf(Phase) <> -1 Then Continue
		    Var NewPermuation() As Integer
		    For Each Element As Integer In BestConfiguration
		      NewPermuation.AddRow Element
		    Next
		    
		    Var PendingElements As Integer = Phases.Count - BestConfiguration.Count
		    For I As Integer = 0 To PendingElements - 1
		      NewPermuation.AddRow Phase
		    Next
		    
		    PendingPermutations.AddRow NewPermuation
		  Next
		  
		  CheckNextPendingPermutation
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub CheckNextPendingPermutation()
		  If PendingPermutations.Count = 0 Then
		    ShowBestConfigurationResult
		    If BestConfiguration.Count = 5 Then Return
		    BestConfiguration.AddRow(NextPhase)
		    AutoCalibrateThrusters(False)
		  End If
		  
		  Var CurrentPermutation() As Integer = PendingPermutations(0)
		  PendingPermutations.RemoveRowAt(0)
		  
		  AmplifierContainerControl1.LoadProgram Program, CurrentPermutation(0)
		  AmplifierContainerControl2.LoadProgram Program, CurrentPermutation(1)
		  AmplifierContainerControl3.LoadProgram Program, CurrentPermutation(2)
		  AmplifierContainerControl4.LoadProgram Program, CurrentPermutation(3)
		  AmplifierContainerControl5.LoadProgram Program, CurrentPermutation(4)
		  
		  AmplifierContainerControl1.AddInput 0
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub ShowBestConfigurationResult()
		  OutputResultLabel.Value = "⚡️" + HighestOutputSignal.ToString
		End Sub
	#tag EndMethod


	#tag Property, Flags = &h0
		BestConfiguration() As Integer
	#tag EndProperty

