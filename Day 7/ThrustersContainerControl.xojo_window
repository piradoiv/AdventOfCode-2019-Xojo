#tag Events FeedbackLoopCheckBox
	#tag Event
		Sub Action()
		  FeedbackLoopVerticalLine.Visible = Me.Value
		  FeedbackLoopHorizontalLine.Visible = Me.Value
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events AutoCalibratePushButton
	#tag Event
		Sub Action()
		  Var d As New CsvDialog
		  d.Label = "Please enter your SantaVM program:"
		  d.Csv = If(Program <> "", Program, DefaultProgram)
		  d.ShowModalWithin Self.Window
		  Program = d.Csv
		  If Program = "" Then Return
		  'AutoCalibrateThrusters
		  
		  AmplifierContainerControl1.LoadProgram Program, 4
		  AmplifierContainerControl2.LoadProgram Program, 3
		  AmplifierContainerControl3.LoadProgram Program, 2
		  AmplifierContainerControl4.LoadProgram Program, 1
		  AmplifierContainerControl5.LoadProgram Program, 0
		  
		  AmplifierContainerControl1.AddInput 0
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events LoadProgramPushButton
	#tag Event
		Sub Action()
		  Var d As New CsvDialog
		  d.Label = "Please enter your SantaVM program:"
		  d.Csv = If(Program <> "", Program, DefaultProgram)
		  d.ShowModalWithin Self.Window
		  Program = d.Csv
		  
		  If Program = "" Then Return
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events PushButton1
	#tag Event
		Sub Action()
		  ThrusterCalibrator1.LoadProgram Program
		  ThrusterCalibrator1.FeedbackLoopEnabled = FeedbackLoopCheckBox.Value
		  ThrusterCalibrator1.Calibrate 5
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events ThrusterCalibrator1
	#tag Event
		Sub ResultAvailable(Sequence() As Integer, Output As Integer)
		  Var SequenceStr As String
		  HighestOutputSignal = Output
		  AmplifierContainerControl1.Phase = Sequence(0)
		  AmplifierContainerControl2.Phase = Sequence(1)
		  AmplifierContainerControl3.Phase = Sequence(2)
		  AmplifierContainerControl4.Phase = Sequence(3)
		  AmplifierContainerControl5.Phase = Sequence(4)
		  ShowBestConfigurationResult
		End Sub
	#tag EndEvent
#tag EndEvents
